"rules": [
    {
        "description": "Original keybinds",
        "manipulators": [
            {
                "from": {
                    "key_code": "d",
                    "modifiers": {
                        "mandatory": [
                            "command"
                        ],
                        "optional": [
                            "caps_lock"
                        ]
                    }
                },
                "to": [
                    {
                        "key_code": "right_arrow",
                        "modifiers": [
                            "right_option"
                        ]
                    },
                    {
                        "key_code": "left_arrow",
                        "modifiers": [
                            "right_option",
                            "shift"
                        ]
                    }
                ],
                "type": "basic",
                "conditions": [
                    {
                      "type": "frontmost_application_unless",
                      "bundle_identifiers": ["^com\\.github\\.atom"]
                    }
                ]
            },
            {
                "from": {
                    "key_code": "spacebar",
                    "modifiers": {
                        "mandatory": [
                            "control"
                        ]
                    }
                },
                "to": [
                    {
                        "key_code": "tab",
                        "modifiers": [
                            "command"
                        ]
                    }
                ],
                "type": "basic"
            },
            {
                "from": {
                    "key_code": "spacebar",
                    "modifiers": {
                        "mandatory": [
                            "control",
                            "command"
