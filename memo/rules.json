    {
        "description": "Original reform keybinds",
        "manipulators": [
            {
                "from": {
                    "key_code": "tab",
                    "modifiers": {
                        "optional": [
                            "command",
                            "shift"
                        ]
                    }
                },
                "to": [
                    {
                        "key_code": "command"
                    }
                ],
                "type": "basic",
                "conditions": [
                    {
                      "type": "frontmost_application_unless",
                      "bundle_identifiers": ["^com\\.google\\.Chrome"]
                    }
                ]
            },
            {
                "from": {
                    "key_code": "tab",
                    "modifiers": {
                        "mandatory": [
                            "command"
                        ],
                        "optional": [
                            "shift"
                        ]
                    }
